#include <bits/stdc++.h>
#include "Particle.h"

using namespace std;

// For World::add_particle,
void test(P& p1, P& p2) {
// This will work
	list<P*> pps;
	pps.push_back(&p1);
	p2 = *(pps.front());

/* This won't work:
	list<P> ps;
	ps.push_back(p1);
	p2 = ps.front();
*/
}


int main() {
//	Particle p(0,0,0,0,0,air);
	Dust d;
	Air a;
	test(d, a);
	d.touch(a);

}
